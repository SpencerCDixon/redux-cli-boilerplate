{
  "name": "redux-cli-boilerplate",
  "version": "0.0.1",
  "description": "Simple React/Redux boilerplate to complement redux-cli for faster development",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "build": "NODE_ENV=production webpack --config ./webpack.prod.config.js --progress",
    "clean": "rimraf dist",
    "deploy": "npm run lint && npm run clean && npm run build",
    "lint": "eslint src/**/*.js",
    "test": "CONFIG=$(pwd)/webpack.test.config.js BABEL_DISABLE_CACHE=1 NODE_ENV=test ava"
  },
  "author": "Spencer C. Dixon",
  "homepage": "https://github.com/SpencerCDixon/redux-cli-boilerplate",
  "bugs": "https://github.com/SpencerCDixon/redux-cli-boilerplate/issues",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "ava": "^0.15.2",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.0.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-webpack-loaders": "^0.7.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "block-loader": "^2.1.0",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.13.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.21.0",
    "json-loader": "^0.5.4",
    "postcss-advanced-variables": "^1.2.2",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.3.3",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  },
  "dependencies": {
    "axios": "^0.12.0",
    "classnames": "^2.2.5",
    "normalize.css": "^4.1.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0-alpha.1",
    "redux": "^3.5.2",
    "redux-form": "^6.0.0-alpha.13",
    "redux-thunk": "^2.1.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "files": [
      "src/**/*.test.js"
    ],
    "verbose": true,
    "failFast": true
  },
  "keywords": [
    "React",
    "Redux",
    "CSS Modules",
    "Hot Module Reloading",
    "Boilerplate"
  ]
}
