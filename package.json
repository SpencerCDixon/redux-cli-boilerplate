    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.1",
    "eslint": "^2.13.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.5.0",
